<Window x:Class="Gemo_test_application.AddResults"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gemo_test_application"
        mc:Ignorable="d"
        Title="AddResults" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Введите результат" FontSize="15"></TextBlock>
            <TextBox Name="txtResult" FontSize="15" Margin="0 10 0 0" PreviewTextInput="txtResult_PreviewTextInput"/>
            <Button Name="btnAddResult" FontSize="15" Margin="0 10 0 0" Content="Принять" Click="btnAddResult_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
